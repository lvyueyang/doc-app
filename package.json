{
  "name": "kangmi-doc",
  "private": true,
  "description": "康米文档，免费的文档工具，支持流程图，脑图，Markdown",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "eslint": "eslint --ext .jsx,.js,.cjs,.mjs,.ts,.tsx --fix",
    "lint": "npm run eslint && npm run prettier",
    "prepare": "husky install",
    "prettier": "prettier --write ./packages/saturn_tenant"
  },
  "lint-staged": {
    "**/*.{tsx,ts,jsx,js}": "npm run lint"
  },
  "dependencies": {
    "@antv/x6": "^2.0.8",
    "@antv/x6-plugin-clipboard": "^2.1.3",
    "@antv/x6-plugin-dnd": "^2.0.3",
    "@antv/x6-plugin-export": "^2.1.5",
    "@antv/x6-plugin-history": "^2.1.2",
    "@antv/x6-plugin-keyboard": "^2.1.4",
    "@antv/x6-plugin-minimap": "^2.0.2",
    "@antv/x6-plugin-scroller": "^2.0.7",
    "@antv/x6-plugin-selection": "^2.1.5",
    "@antv/x6-plugin-snapline": "^2.1.6",
    "@antv/x6-plugin-transform": "^2.1.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "antd": "^5.0.7"
  },
  "devDependencies": {
    "@types/node": "17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lerna": "^6.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "typescript": "^4.1.2",
    "umi": "^4.0.36"
  }
}
